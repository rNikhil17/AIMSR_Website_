@model IEnumerable<AIMSR_Website.Models.Student>

<h2>Student List</h2>

<table border="1">
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Course</th>
    </tr>
    @foreach (var student in Model)
    {
        <tr>
            <td>@student.Name</td>
            <td>@student.Email</td>
            <td>@student.Course</td>
        </tr>
    }
</table>
